  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "start": "npm run build && vite preview --port 4173",
    "test:unit": "vitest",
    "test:e2e": "cross-env CYPRESS_VIDEO=true start-server-and-test start http://localhost:4173 cy:run",
    "cy:run": "cypress run --e2e --browser edge --",
    "cy:run:video": "cypress run --no-runner-ui --e2e --browser edge --",
    "cypress:open": "cypress open --e2e",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "generate:api": "orval --config orval.config.ts"
  },
